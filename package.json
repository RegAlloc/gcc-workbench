{
  "name": "gcc-syntax-highlighting",
  "displayName": "gcc-syntax-highlighting",
  "description": "Syntax highlighting for GCC RTL, Gimple, Match.pd, and MD files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gcc-md",
        "aliases": ["gcc machine description"],
        "extensions": [".md"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "gcc-rtl",
        "aliases": ["gcc rtl dump"],
        "filenamePatterns": ["**/*.[0-9][0-9][0-9]r.*"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "gcc-gimple",
        "aliases": ["gcc gimple dump"],
        "filenamePatterns": ["**/*.[0-9][0-9][0-9]t.*", "**/*.[0-9][0-9][0-9]i.*"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "gcc-match",
        "aliases": ["gcc matchpd"],
        "extensions": [".pd"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gcc-md",
        "scopeName": "source.gcc.md",
        "path": "./syntaxes/md.tmLanguage.json"
      },
      {
        "language": "gcc-rtl",
        "scopeName": "source.gcc.rtl",
        "path": "./syntaxes/rtl.tmLanguage.json"
      },
      {
        "language": "gcc-gimple",
        "scopeName": "source.gcc.gimple",
        "path": "./syntaxes/gimple.tmLanguage.json"
      },
      {
        "language": "gcc-match",
        "scopeName": "source.gcc.match",
        "path": "./syntaxes/match.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[gcc-md]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      },
      "[gcc-match]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      }
    }
  }
}
