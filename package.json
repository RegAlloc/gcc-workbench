{
  "name": "gcc-syntax-highlighting",
  "displayName": "GCC Workbench (RTL, GIMPLE & MD)",
  "description": "The professional workspace for GCC engineers. Filter noise, and navigate Machine description files, RTL/GIMPLE dumps.",
  "version": "2.1.0",
  "author": {
    "name": "Kishan Parmar",
    "email": "kparmar2101@gmail.com",
    "url": "https://github.com/RegAlloc"
  },
  "publisher": "RegAlloc",
  "icon": "pictures/gcc.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/RegAlloc/gcc-workbench/"
  },
  "bugs": {
    "url": "https://github.com/RegAlloc/gcc-workbench/issues"
  },
  "homepage": "https://github.com/RegAlloc/gcc-workbench/",
  "_license": {
    "name": "GPL-3.0-or-later",
    "copyright": "Copyright (C) 2025 Kishan Parmar",
    "notice": "This file is free software: you can redistribute it and/or modify it under the terms of the GNU General Public License as published by the Free Software Foundation; either version 3 of the License, or (at your option) any later version. This file is distributed WITHOUT ANY WARRANTY. See https://www.gnu.org/licenses/ for details."
  },
  "keywords": [
    "gcc",
    "gnu",
    "rtl",
    "gimple",
    "compiler",
    "powerpc",
    "aarch64",
    "toolchain"
  ],
  "engines": {
    "vscode": "^1.50.0"
  },
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "categories": [
    "Programming Languages",
    "Visualization",
    "Snippets",
    "Language Packs"
  ],
  "activationEvents": [
    "onLanguage:gcc-md",
    "onLanguage:gcc-rtl",
    "onLanguage:gcc-gimple",
    "onLanguage:gcc-match",
    "onLanguage:rs6000-builtins",
    "onLanguage:rs6000-overload",
    "onView:gcc-dump-explorer",
    "onCommand:gcc-workbench.viewSource"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "gcc-md",
        "path": "./snippets/gcc-md.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gcc-explorer",
          "title": "GCC Explorer",
          "icon": "./pictures/gnu-dark.png"
        }
      ]
    },
    "views": {
      "gcc-explorer": [
        {
          "id": "gcc-dump-explorer",
          "name": "Compilation Passes"
        }
      ]
    },
    "commands": [
      {
        "command": "gcc-md.openFilePermanent",
        "title": "Open MD Include"
      },
      {
        "command": "gcc-dump.comparePreviousPass",
        "title": "Compare with Previous Pass",
        "icon": "$(diff-multiple)"
      },
      {
        "command": "gcc-dump.refreshPasses",
        "title": "Refresh Passes",
        "icon": "$(refresh)"
      },
      {
        "command": "gcc-dump.filterPasses",
        "title": "Filter Categories",
        "icon": "$(filter)"
      },
      {
        "command": "gcc-dump.toggleFocus",
        "title": "Hide Noise",
        "icon": "$(eye-closed)"
      },
      {
        "command": "gcc-dump.toggleFocus_on",
        "title": "Show Noise",
        "icon": "$(eye)"
      },
      {
        "command": "gcc-dump.openDotFile",
        "title": "Open Graph (CFG)",
        "icon": "$(layers-dot)"
      },
      {
        "command": "gcc-dump.nextPass",
        "title": "Next Pass",
        "icon": "$(arrow-right)"
      },
      {
        "command": "gcc-dump.prevPass",
        "title": "Previous Pass",
        "icon": "$(arrow-left)"
      },
      {
        "command": "gcc-workbench.viewSource",
        "title": "View Source (Split)",
        "icon": "$(file-code)"
      }
    ],
    "keybindings": [
      {
        "command": "gcc-dump.prevPass",
        "key": "ctrl+alt+[",
        "mac": "ctrl+alt+[",
        "when": "editorTextFocus && resourceFilename =~ /\\.[0-9]{3}[tir]\\.[^.]+$/"
      },
      {
        "command": "gcc-dump.nextPass",
        "key": "ctrl+alt+]",
        "mac": "ctrl+alt+]",
        "when": "editorTextFocus && resourceFilename =~ /\\.[0-9]{3}[tir]\\.[^.]+$/"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "gcc-dump.comparePreviousPass",
          "when": "resourceFilename =~ /\\.[0-9]{3}[tir]\\./",
          "group": "navigation@1"
        },
        {
          "command": "gcc-dump.toggleFocus",
          "when": "resourceFilename =~ /\\.[0-9]{3}[tir]\\.[^.]+$/ && !gcc-dump.focusModeActive",
          "group": "navigation@2"
        },
        {
          "command": "gcc-dump.toggleFocus_on",
          "when": "resourceFilename =~ /\\.[0-9]{3}[tir]\\.[^.]+$/ && gcc-dump.focusModeActive",
          "group": "navigation@2"
        },
        {
          "command": "gcc-dump.openDotFile",
          "when": "resourceFilename =~ /\\.[0-9]{3}[tir]\\.[^.]+$/",
          "group": "navigation@3"
        },
        {
          "command": "gcc-dump.prevPass",
          "group": "navigation@4",
          "when": "editorLangId == gcc-rtl || editorLangId == gcc-gimple"
        },
        {
          "command": "gcc-dump.nextPass",
          "group": "navigation@5",
          "when": "editorLangId == gcc-rtl || editorLangId == gcc-gimple"
        },
        {
          "command": "gcc-workbench.viewSource",
          "group": "navigation",
          "when": "resourceLangId =~ /gcc-rtl/"
        }
      ],
      "view/title": [
        {
          "command": "gcc-dump.refreshPasses",
          "when": "view == gcc-dump-explorer",
          "group": "navigation"
        },
        {
          "command": "gcc-dump.filterPasses",
          "when": "view == gcc-dump-explorer",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "rs6000-builtins",
        "aliases": [
          "GCC RS6000 Builtins"
        ],
        "filenames": [
          "rs6000-builtins.def"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/rs6000.png",
          "dark": "./pictures/rs6000.png"
        }
      },
      {
        "id": "rs6000-overload",
        "aliases": [
          "GCC RS6000 Overloads"
        ],
        "filenames": [
          "rs6000-overload.def"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/rs6000.png",
          "dark": "./pictures/rs6000.png"
        }
      },
      {
        "id": "gcc-md",
        "aliases": [
          "GCC Machine Description",
          "gcc-md"
        ],
        "extensions": [
          ".md"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      },
      {
        "id": "gcc-rtl",
        "aliases": [
          "GCC RTL Dump"
        ],
        "filenamePatterns": [
          "**/*.[0-9][0-9][0-9]r.[^.]*"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      },
      {
        "id": "gcc-gimple",
        "aliases": [
          "GCC Gimple"
        ],
        "filenamePatterns": [
          "**/*.[0-9][0-9][0-9]t.[^.]*",
          "**/*.[0-9][0-9][0-9]i.[^.]*"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      },
      {
        "id": "gcc-match",
        "aliases": [
          "GCC MatchPD"
        ],
        "extensions": [
          ".pd"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      },
      {
        "id": "gcc-exp",
        "aliases": [
          "GCC Expression"
        ],
        "extensions": [
          ".exp"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      },
      {
        "id": "gcc-opt",
        "aliases": [
          "GCC Options"
        ],
        "extensions": [
          ".opt"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gnu-dark.png",
          "dark": "./pictures/gnu-light.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "rs6000-builtins",
        "scopeName": "source.gcc.rs6000",
        "path": "./syntaxes/rs6000-builtins.tmLanguage.json"
      },
      {
        "language": "rs6000-overload",
        "scopeName": "source.gcc.rs6000-overload",
        "path": "./syntaxes/rs6000-overload.tmLanguage.json"
      },
      {
        "language": "gcc-md",
        "scopeName": "source.gcc.md",
        "path": "./syntaxes/md.tmLanguage.json"
      },
      {
        "language": "gcc-rtl",
        "scopeName": "source.gcc.rtl",
        "path": "./syntaxes/rtl.tmLanguage.json"
      },
      {
        "language": "gcc-gimple",
        "scopeName": "source.gcc.gimple",
        "path": "./syntaxes/gimple.tmLanguage.json"
      },
      {
        "language": "gcc-match",
        "scopeName": "source.gcc.match",
        "path": "./syntaxes/match.tmLanguage.json"
      },
      {
        "language": "gcc-exp",
        "scopeName": "source.gcc.exp",
        "path": "./syntaxes/exp.tmLanguage.json"
      },
      {
        "language": "gcc-opt",
        "scopeName": "source.gcc.opt",
        "path": "./syntaxes/opt.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[gcc-md]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      },
      "[gcc-match]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      },
      "files.associations": {
        "t-*": "makefile",
        "x-*": "makefile",
        "config.in": "cpp",
        "config.*": "shellscript",
        "*.dot": "dot",
        "*.svg": "svg"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.50.0",
    "typescript": "^5.9.3"
  }
}
