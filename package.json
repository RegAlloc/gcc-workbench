{
  "name": "gcc-syntax-highlighting",
  "displayName": "GCC Syntax Highlighting Toolkit (RTL, GIMPLE & MD)",
  "description": "Syntax highlighting for GCC RTL, Gimple, Match.pd, and Machine Description files",
  "version": "0.1.1",
  "publisher": "RegAlloc",
  "icon": "pictures/gcc.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/RegAlloc/gcc-syntax-highlighting/"
  },
  "bugs": {
    "url": "https://github.com/RegAlloc/gcc-syntax-highlighting/issues"
  },
  "homepage": "https://github.com/RegAlloc/gcc-syntax-highlighting/",
  "_license": {
    "name": "GPL-3.0-or-later",
    "copyright": "Copyright (C) 2025 Kishan Parmar",
    "notice": "This file is free software: you can redistribute it and/or modify it under the terms of the GNU General Public License as published by the Free Software Foundation; either version 3 of the License, or (at your option) any later version. This file is distributed WITHOUT ANY WARRANTY. See https://www.gnu.org/licenses/ for details."
  },
  "keywords": [
    "gcc",
    "gnu",
    "rtl",
    "gimple",
    "compiler",
    "powerpc",
    "aarch64",
    "toolchain"
  ],
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gcc-md",
        "aliases": [
          "GCC Machine Description",
          "gcc-md"
        ],
        "extensions": [
          ".md"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gcc.png",
          "dark": "./pictures/gcc.png"
        }
      },
      {
        "id": "gcc-rtl",
        "aliases": [
          "GCC RTL Dump"
        ],
        "filenamePatterns": [
          "**/*.[0-9][0-9][0-9]r.*"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gcc.png",
          "dark": "./pictures/gcc.png"
        }
      },
      {
        "id": "gcc-gimple",
        "aliases": [
          "GCC Gimple"
        ],
        "filenamePatterns": [
          "**/*.[0-9][0-9][0-9]t.*",
          "**/*.[0-9][0-9][0-9]i.*"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gcc.png",
          "dark": "./pictures/gcc.png"
        }
      },
      {
        "id": "gcc-match",
        "aliases": [
          "GCC MatchPD"
        ],
        "extensions": [
          ".pd"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./pictures/gcc.png",
          "dark": "./pictures/gcc.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "gcc-md",
        "scopeName": "source.gcc.md",
        "path": "./syntaxes/md.tmLanguage.json"
      },
      {
        "language": "gcc-rtl",
        "scopeName": "source.gcc.rtl",
        "path": "./syntaxes/rtl.tmLanguage.json"
      },
      {
        "language": "gcc-gimple",
        "scopeName": "source.gcc.gimple",
        "path": "./syntaxes/gimple.tmLanguage.json"
      },
      {
        "language": "gcc-match",
        "scopeName": "source.gcc.match",
        "path": "./syntaxes/match.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[gcc-md]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      },
      "[gcc-match]": {
        "editor.tabSize": 8,
        "editor.detectIndentation": false,
        "editor.action.changeTabDisplaySize": 8
      }
    }
  }
}
